@model List<Usuario>
<h1>Usuário cadastrado no sistema</h1>
<p class="text-dager">@ViewData["mensagem"]</p>
<table class="table">
  <thead>
    <tr>
      <th scope="row">Nome</th>
      <th scope="row">Login</th>
      <th scope="row">Tipo</th>
    </tr>
  </thead>

  <tbody>
    @foreach (Usuario u in Model)
    {
      <td>@u.Nome</td>
      <td>@u.login</td>
      @if (u.tipo == Usuario.ADMIN)
      {
        <td>Administrador</td>
      }
      else
      {
        <td>Padrão</td>
      }
      <td><a href="editarUsuario?id=@u.Id">editar</a></td>
      <td><a href="ExcluirUsuario?id=@u.Id">excluirr</a></td>
    }
  </tbody>
</table>
<a href="RegistrarUsuario">Registra novo usuario</a>
